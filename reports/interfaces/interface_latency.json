{
    "name": "Interface Average Latency",
    "category": "Interfaces",
    "description": "Interface average latency over time",
    "displayOrder": 20,
    "type": "CATEGORIES_SERIES",
    "table": "interface_stats",
    "units": "ms",
    "queryCategories": {
        "groupColumn": "interface_id",
        "aggregationFunction": "avg",
        "aggregationValue": "avg_latency/1000000",
        "limit": 10
    },
    "querySeries": {
        "timeIntervalSeconds": 60
    },
    "rendering": {
        "type": "spline",
        "lineWidth": 2,
        "dataGroupingApproximation": "average"
    }
}
